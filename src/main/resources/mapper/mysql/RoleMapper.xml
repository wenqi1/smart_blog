<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.learn.blog.dao.RoleDao" >

    <select id="queryRoleById" resultType="com.learn.blog.model.Role">
        select * from wq_role where id = cast(#{id} as numeric )
    </select>

