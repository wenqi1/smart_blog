<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.learn.blog.dao.TagDao" >

    <select id="queryTagById" resultType="com.learn.blog.model.Tag">
        select * from wq_tag where id = cast(#{id} as numeric )
    </select>

</mapper>